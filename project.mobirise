          "_customHTML": "<section class=\"features7\" group=\"Features\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"mobirise-icons2\">\n    <!---->\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"10\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"8\">\n        <input type=\"range\" name=\"imageWidth\" title=\"Image\" min=\"3\" max=\"8\" step=\"1\" value=\"3\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Main Title\" name=\"showmainTitle\" checked>\n        <input type=\"range\" name=\"items\" title=\"Items\" min=\"1\" max=\"3\" step=\"1\" value=\"3\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Link\" name=\"showLink\" checked>\n        <input type=\"color\" title=\"Color\" name=\"cardsBg\" value=\"#191919\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1-1.jpg\" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#040402\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#bed3f9\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid' : fullWidth}\">\n\n        <div class=\"row\">\n            <div class=\"col-12\">\n                <h2 class=\"mbr-maintitle align-left mbr-fonts-style pb-4\" mbr-theme-style=\"display-2\" mbr-if=\"showmainTitle\"><b>Resources</b></h2>\n            </div>\n        </div>\n\n        <!-- Project 1-->\n        <div class=\"card-wrapper\">\n            <div class=\"row card-row align-items-center\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n                <div class=\"col-12 img-col col-lg-{{imageWidth}} col-md-6\">\n                    <div class=\"image-wrapper\">\n                        <a href=\"https://www.youtube.com/watch?v=GEpH3o44_58\"><img src=\"@PROJECT_PATH@/assets/images/using-the-metahuman-facial-rig-in-ue-453x257.png\" alt=\"Mobirise\"></a>\n                    </div>\n                </div>\n                <div class=\"col-12 col-lg col-md-6\">\n                    <div class=\"text-box\">\n                        <h4 class=\"mbr-title align-left mbr-fonts-style pb-2\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\"><b>Unreal, Animation</b></h4>\n                        <p class=\"mbr-text align-left mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">Using the MetaHuman Facial Rig in Unreal Engine</p>\n                        <h5 class=\"link align-right mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showLink\" data-app-selector=\".link\"><a href=\"https://www.youtube.com/watch?v=GEpH3o44_58\" class=\"text-primary\">Read more</a></h5>\n                    </div>\n\n                </div>\n            </div>\n        </div>\n        \n        <!-- Project 2-->\n        <div class=\"card-wrapper\" mbr-if=\"items > 1\">\n            <div class=\"row card-row align-items-center\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n                <div class=\"col-12 img-col col-lg-{{imageWidth}} col-md-6\">\n                    <div class=\"image-wrapper\">\n                        <a href=\"https://www.youtube.com/watch?v=O3gLBhC353Y\"><img src=\"@PROJECT_PATH@/assets/images/blender-procedural-nodes-568x291.png\" alt=\"Mobirise\"></a>\n                    </div>\n                </div>\n                <div class=\"col-12 col-lg col-md-6\">\n                    <div class=\"text-box\">\n                        <h4 class=\"mbr-title align-left mbr-fonts-style pb-2\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\"><b>Blender, Procedural Modeling</b></h4>\n                        <p class=\"mbr-text align-left mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">Blender's Procedural Nodes&nbsp;</p>\n                        <h5 class=\"link align-right mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showLink\" data-app-selector=\".link\"><a href=\"https://www.youtube.com/watch?v=O3gLBhC353Y\" class=\"text-primary\">Read more</a></h5>\n                    </div>\n\n                </div>\n            </div>\n        </div>\n\n        <!-- Project 3-->\n        <div class=\"card-wrapper\" mbr-if=\"items > 2\">\n            <div class=\"row card-row align-items-center\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n                <div class=\"col-12 img-col col-lg-{{imageWidth}} col-md-6\">\n                    <div class=\"image-wrapper\">\n                        <a href=\"https://www.uxofvr.com\"><img src=\"@PROJECT_PATH@/assets/images/firefox-tezr3y9g86-568x345.png\" alt=\"Mobirise\"></a>\n                    </div>\n                </div>\n                <div class=\"col-12 col-lg col-md-6\">\n                    <div class=\"text-box\">\n                        <h4 class=\"mbr-title align-left mbr-fonts-style pb-2\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\">\n                            <b>UI/UX, VR</b></h4>\n                        <p class=\"mbr-text align-left mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">The UX of VR</p>\n                        <h5 class=\"link align-right mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showLink\" data-app-selector=\".link\"><a href=\"https://www.uxofvr.com\" class=\"text-primary\">Read more</a></h5>\n                    </div>\n\n                </div>\n            </div>\n        </div>\n        <!-- Project 4-->\n\n        <!-- ENDOF Project 4-->\n        <!-- Project 5-->\n\n        <!-- ENDOF Project 5-->\n        <!-- Project 6-->\n\n        <!-- ENDOF Project 6-->\n        <!-- Project 7-->\n\n        <!-- ENDOF Project 7-->\n    </div>\n</section>",
